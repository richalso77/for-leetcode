class Solution {
public:
    long long dividePlayers(vector<int>& skill) {
        int n = skill.size();
        if (n % 2)  return -1;
        std::sort(skill.begin(), skill.end());
        long long sum = 0;
        int check = skill[0] + skill[n - 1];
        for (int i = 0; i < n / 2; i++){
            if (skill[i] + skill[n - i - 1] != check)   return -1;
            sum += skill[i] * skill[n - i - 1];
        }
        return sum;
    }
};
